<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Login Or Register</title>
    <link rel="stylesheet" href="/style.css">
         <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
</head>
<body>

<section>
  <div class="imgbox">
      <img src="images/background4.jpg" alt="">
    </div>


  <div class="contentbox-signup">
    <div class="logo">

    </div>
    <div class="formbox-signup">
      <p>
        <h2 class="headline-signup"><a class="clk-signup" href="#">Start Up Register</a></h2>
      </p>

      <form action="/startup" method="POST" >
        <div class="inputbox">
            <span>Full Name</span>
            <input type="text" name="user_name" id="_rname" class="form-control" placeholder="Name"  required>
          </div>
  
          <div class="inputbox">
              <span> start up Name</span>
              <input type="text" name="stname" id="_rname" class="form-control" placeholder="Name"  required>
            </div>
  
            <div class="inputbox">
              <span>Startup Detail</span>
              <input type="text" name="stdetail" id="_rname" class="form-control" placeholder="Dstails"  required>
            </div>
  
            <div class="inputbox">
              <span>No Of People</span>
              <input type="number" name="people" id="_rname" class="form-control" placeholder="No"  required>
            </div>
  

        <div class="inputbox">
          <input type="submit" name="" value="Sign in">
        </div>

        <div class="inputbox">
          
        </div>

      </form>
    </div>
  </div>
</section>
<% if (locals.login_errors) { 
  login_errors.forEach(function(error_msg){ %>
  <div class="alert alert-danger" role="alert"><%= error_msg %></div>
<% });
} %>

</body>
</html>